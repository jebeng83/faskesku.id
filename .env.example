# Application Configuration
APP_NAME="Faskesku.id"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=Asia/Jakarta
APP_URL=http://127.0.0.1:8080

# Sanctum Configuration (stateful domains for SPA auth in dev)
SANCTUM_STATEFUL_DOMAINS=

# Application Locale
APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=127.0.0.1
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=root
DB_PASSWORD=

# BPJS Configuration
BPJS_MOBILEJKN_BASE_URL=https://apijkn.bpjs-kesehatan.go.id/antreanfktp
BPJS_PCARE_BASE_URL=https://apijkn.bpjs-kesehatan.go.id/pcare-rest
BPJS_PCARE_CONS_ID=
BPJS_PCARE_CONS_PWD=
BPJS_PCARE_USER_KEY=
BPJS_PCARE_USER=
BPJS_PCARE_PASS=
BPJS_PCARE_KODE_PPK=
BPJS_PCARE_APP_CODE=095
BPJS_PCARE_KABUPATEN=
# Runtime HTTP options and DNS/IP fallback for PCare
BPJS_HTTP_CONNECT_TIMEOUT=10
BPJS_HTTP_TIMEOUT=30
BPJS_HTTP_RETRY_TIMES=2
BPJS_HTTP_RETRY_SLEEP=500
# SSL verification ENABLED (recommended in production). Ensure PHP trusts CA bundle via curl.cainfo/openssl.cafile in php.ini.
BPJS_HTTP_DISABLE_SSL_VERIFY=false
# Consolidated preferred IPs for apijkn.bpjs-kesehatan.go.id to mitigate intermittent DNS/route issues (working candidates only)
BPJS_PCARE_FORCE_RESOLVE_LIST=
# Alternative SQLite Configuration (uncomment if using SQLite)
# DB_CONNECTION=sqlite
# DB_DATABASE=/absolute/path/to/database.sqlite

# Session Configuration
SESSION_DRIVER=file
SESSION_LIFETIME=120
SESSION_ENCRYPT=true
SESSION_PATH=/
# SESSION_DOMAIN=127.0.0.1  # Commented out for development - allows cookie sharing across ports
SESSION_SECURE_COOKIE=false
SESSION_SAME_SITE=lax
SESSION_HTTP_ONLY=true

# Cache Configuration
CACHE_STORE=database
CACHE_PREFIX=

# Queue Configuration
QUEUE_CONNECTION=database

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=
MAIL_PASSWORD=
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=hello@example.com
MAIL_FROM_NAME=Example

# Alternative Mail Services (uncomment if using)
# MAIL_MAILER=mailgun
# MAILGUN_DOMAIN=your-domain.com
# MAILGUN_SECRET=your-mailgun-secret

# MAIL_MAILER=postmark
# POSTMARK_TOKEN=your-postmark-token

# MAIL_MAILER=ses
# AWS_ACCESS_KEY_ID=your-aws-access-key
# AWS_SECRET_ACCESS_KEY=your-aws-secret-key
# AWS_DEFAULT_REGION=us-east-1
# AWS_BUCKET=your-s3-bucket

# Logging Configuration
LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Broadcasting Configuration
BROADCAST_CONNECTION=log
BROADCAST_DRIVER=log

# Filesystem Configuration
FILESYSTEM_DISK=local

# Redis Configuration (if using Redis)
REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0
REDIS_CACHE_DB=1

# Memcached Configuration (if using Memcached)
MEMCACHED_HOST=127.0.0.1
MEMCACHED_PORT=11211
MEMCACHED_USERNAME=
MEMCACHED_PASSWORD=

# Vite Configuration
VITE_APP_NAME="${APP_NAME}"
VITE_APP_ENV="${APP_ENV}"
VITE_PORT=5177
VITE_DEV_HOSTS=

# Third Party Services
# Google Services
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=
GOOGLE_REDIRECT_URI="${APP_URL}/auth/google/callback"

# Facebook Services
FACEBOOK_CLIENT_ID=
FACEBOOK_CLIENT_SECRET=
FACEBOOK_REDIRECT_URI="${APP_URL}/auth/facebook/callback"

# WhatsApp API (if using)
WHATSAPP_API_URL=
WHATSAPP_API_TOKEN=

# SMS Gateway (if using)
SMS_GATEWAY_URL=
SMS_GATEWAY_USERNAME=
SMS_GATEWAY_PASSWORD=

# Payment Gateway Configuration
# Midtrans
MIDTRANS_SERVER_KEY=
MIDTRANS_CLIENT_KEY=
MIDTRANS_IS_PRODUCTION=false
MIDTRANS_IS_SANITIZED=true
MIDTRANS_IS_3DS=true

# Xendit (alternative payment gateway)
XENDIT_SECRET_KEY=
XENDIT_PUBLIC_KEY=
XENDIT_WEBHOOK_TOKEN=

# File Upload Configuration
MAX_FILE_SIZE=10240
ALLOWED_FILE_TYPES="jpg,jpeg,png,pdf,doc,docx"

# Medical System Specific Configuration
# BPJS Integration (if applicable)
BPJS_CONS_ID=
BPJS_SECRET_KEY=
BPJS_USER_KEY=
BPJS_BASE_URL=https://apijkn-dev.bpjs-kesehatan.go.id

# SATUSEHAT Integration (if applicable)
SATUSEHAT_ENV=DEV
SATUSEHAT_AUTH=
SATUSEHAT_FHIR=
SATUSEHAT_ORG_ID=
SATUSEHAT_CLIENT_ID=
SATUSEHAT_CLIENT_SECRET=

# SETUP ALAMAT & INFORMASI PENDUKUNG FASKES
ALAMAT=
KOTA=
KODE_POS=
KODE_PROVINSI=
KODE_KABUPATEN=
KODE_KECAMATAN=
KODE_KELURAHAN=

WEBSITE=
PHONE=
EMAIL=
LATITUDE=
LONGITUDE=
ALTITUDE=


# Security Configuration
BCRYPT_ROUNDS=12
HASH_VERIFY=true

# API Rate Limiting
API_RATE_LIMIT=60
API_RATE_LIMIT_WINDOW=1

# Backup Configuration (if using backup package)
BACKUP_DISK=local
BACKUP_NOTIFICATION_MAIL=admin@faskesku.id

# Monitoring and Analytics
# Sentry (Error Tracking)
SENTRY_LARAVEL_DSN=
SENTRY_TRACES_SAMPLE_RATE=1.0

# Google Analytics
GOOGLE_ANALYTICS_ID=

# Development Tools
TELESCOPE_ENABLED=true
DEBUGBAR_ENABLED=true

# Maintenance Mode
APP_MAINTENANCE_DRIVER=file
APP_MAINTENANCE_STORE=database

URUT_NO_REG=
OCTANE_SERVER=
AKTIFKANBATCHOBAT=No
